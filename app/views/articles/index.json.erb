[
<% @articles.each_with_index do |article, index| %>
  {
    "id": <%= article.id %>,
    "title": "<%= article.title %>",
    "content": "<%= article.content %>",
    "createdAt": "<%= article.created_at %>",
    "updatedAt": "<%= article.updated_at %>"
  <%= index == (@articles.length - 1) ? '}' : '},'%>
<% end %>
]
