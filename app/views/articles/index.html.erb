<h1>Article Index</h1>
<%= link_to "New Article", new_article_path %>
<table>
  <tr>
    <th>#</th>
    <th>title</th>
    <th>content</th>
    <th>created at</th>
    <th>updated at</th>
    <th>actions</th>
  </tr>
  <% @articles.each do |article| %>
  <tr>
    <td><%= article.id %></td>
    <td><%= article.title %></td>
    <td><%= article.content %></td>
    <td><%= article.created_at %></td>
    <td><%= article.updated_at %></td>
    <td>
      <%= link_to "Show", article_path(article.id) %>
      <%= link_to "Edit", edit_article_path(article.id) %>
    </td>
  </tr>
  <% end %>
</table>
